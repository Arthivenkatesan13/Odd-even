class HelloWorld {
    public static void main(String[] args) {
        
        int a[]={1,2,3,4,7,8,9};
        System.out.print("odd no:");
        for(int i=0;i<a.length;i++)
        if(a[i]%2!=0)
        System.out.print(a[i]);
        System.out.println();
        System.out.print("even no:");
        for(int i=0;i<a.length;i++)
         if(a[i]%2==0)
        System.out.print(a[i]);

       
}
}
